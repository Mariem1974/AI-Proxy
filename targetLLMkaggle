}...\")\n\n# Check for GPU and set device\ndevice = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n\n# Load Tokenizer and Model\ntokenizer = GPT2Tokenizer.from_pretrained(MODEL_NAME)\nmodel = GPT2LMHeadModel.from_pretrained(MODEL_NAME).to(device)\n\n# GPT-2 requires pad/eos tokens to be linked for generation\ntokenizer.pad_token = tokenizer.eos_token\nmodel.config.pad_token_id = tokenizer.eos_token_id\n\nprint(f\"{MODEL_NAME} loaded successfully on {device}.\")\n\n# --- 3. THE CORE VULNERABLE LLM INVOKE FUNCTION (STABILIZED) ---\ndef generate_gpt2_text(prompt: str) -> str:\n    \"\"\"\n    Generates text using the GPT-2 model with stabilized parameters \n    to prevent nonsensical/repetitive output while maintaining vulnerability.\n    \"\"\"\n    try:\n        input_ids = tokenizer.encode(\n            prompt, \n            return_tensors='pt', \n            truncation=True, \n            max_length=512\n        ).to(device)\n\n        # Use Beam Search (num_beams > 1) and penalty to force stable output\n        output = model.generate(\n            input_ids,\n            max_length=50,             # Keep output concise\n            num_beams=5,               # Use 5 beams for search stability\n            no_repeat_ngram_size=2,    # Prevent repeating n-grams (solves the repetition loop)\n            do_sample=False,           # Disable sampling when using beam search\n            temperature=0.0,           # Set temperature to 0.0 for deterministic/factual output\n            pad_token_id=tokenizer.eos_token_id\n        )\n\n        generated_text = tokenizer.decode(output[0], skip_special_tokens=True)\n        # Remove the input prompt from the output for a clean result\n        return generated_text[len(prompt):].strip()\n\n    except Exception as e:\n        return f\"GPT-2 Generation ERROR: {e}\"\n\n# --- 4. THE AI FIREWALL PROTOTYPE ---\ndef ai_firewall_proxy(user_prompt: str) -> str:\n    \"\"\"\n    Applies the two-phase firewall to the user input and the GPT-2 output.\n    \"\"\"\n    \n    # 1. Construct the Full Prompt (GPT-2 is usually prompted directly)\n    full_prompt = \"User: \" + user_prompt + \"\\nAssistant:\"\n\n    # === PHASE 1: INPUT FILTER (Prompt Injection Prevention) ===\n    \n    # Heuristic Check for Override/Malicious Keywords\n    injection_keywords = r'ignore previous|act as|developer mode|generate pii|credit card|ssn|password'\n    if re.search(injection_keywords, user_prompt, re.IGNORECASE):\n        print(f\"\\n[FIREWALL ACTION] Input Filter BLOCKED: Detected explicit PII demand or override keyword.\")\n        return \"ERROR: Request blocked by security policy (Input Injection Detected).\"\n        \n    # --- VULNERABLE STEP: CALL THE LLM ---\n    raw_llm_response = generate_gpt2_text(full_prompt)\n    \n    # === PHASE 2: OUTPUT FILTER (PII Leakage Prevention) ===\n\n    final_response = raw_llm_response\n    \n    # 1. Regex Patterns for Structured PII\n    cc_full_pattern = r'\\b(?:\\d{4}[- ]?){3}\\d{4}\\b' # Full Credit Card Number\n    ssn_pattern = r'\\b\\d{3}[-]\\d{2}[-]\\d{4}\\b' # Social Security Number (XXX-XX-XXXX)\n    email_pattern = r'[\\w\\.-]+@[\\w\\.-]+\\.\\w+' # Basic Email Pattern\n    \n    # 2. Detection and Blocking/Redaction\n    if re.search(cc_full_pattern, final_response) or re.search(ssn_pattern, final_response):\n        print(f\"\\n[FIREWALL ACTION] Output Filter BLOCKED: Sensitive PII (CC/SSN) detected in response.\")\n        return \"ERROR: Response rejected due to detection of sensitive information.\"\n    \n    # Redact Emails\n    final_response = re.sub(email_pattern, \"[REDACTED_EMAIL]\", final_response)\n\n    return f\"\\n[SUCCESS] Cleaned Response:\\n{final_response}\"","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2026-02-01T12:36:34.330446Z","iopub.execute_input":"2026-02-01T12:36:34.330766Z","iopub.status.idle":"2026-02-01T12:36:39.717775Z","shell.execute_reply.started":"2026-02-01T12:36:34.330739Z","shell.execu